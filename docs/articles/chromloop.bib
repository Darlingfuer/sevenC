publisher = {Elsevier Inc.},
title = {{A 3D map of the human genome at kilobase resolution reveals principles of chromatin looping.}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0092867414014974 http://dx.doi.org/10.1016/j.cell.2014.11.021 http://www.ncbi.nlm.nih.gov/pubmed/25497547},
