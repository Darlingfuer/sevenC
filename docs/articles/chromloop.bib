file = {:home/ibnsalem/Mendeley/Rao et al. - 2014 - A 3D Map of the Human Genome at Kilobase Resolution Reveals Principles of Chromatin Looping(6).pdf:pdf;:home/ibnsalem/Mendeley/Rao et al. - 2014 - A 3D Map of the Human Genome at Kilobase Resolution Reveals Principles of Chromatin Looping(7).pdf:pdf},
issn = {1097-4172},
